        $mockObject = $this->getMockObjectGenerator()->getMock(
          $originalClassName,
          $methods,
          $arguments,
          $mockClassName,
          $callOriginalConstructor,
          $callOriginalClone,
          $callAutoload,
          $cloneArguments,
          $callOriginalMethods
        );

        $this->mockObjects[] = $mockObject;

        return $mockObject;
    }

