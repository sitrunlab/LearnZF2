<script type="text/javascript">
function postAjaxCall(url, setViewCallBack, params)
{
    var xmlhttp; // @see http://www.w3schools.com/ajax/ajax_xmlhttprequest_create.asp
    if (window.XMLHttpRequest) { // code for IE7+, Firefox, Chrome, Opera, Safari
        xmlhttp=new XMLHttpRequest();
    } else { // code for IE6, IE5
        xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
    }

    xmlhttp.open("POST", url, true);

    xmlhttp.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
    xmlhttp.setRequestHeader("Accept", "application/json");

    if (params != undefined) {
        xmlhttp.setRequestHeader("Content-length", params.length);
    }

    xmlhttp.setRequestHeader("Connection", "close");
    if (params != undefined) {
        xmlhttp.send(params);
    } else {
        xmlhttp.send(null);
    }

    xmlhttp.onreadystatechange = function() {
        if (xmlhttp.readyState == 4 && xmlhttp.status == 200) {
            setViewCallBack(JSON.parse(xmlhttp.responseText))
        }
    }
}

function chooseRoleGetResourcesAndPermissionLists(el)
{
    var params = "roleId="+el.value;
    postAjaxCall('<?php echo $this->url('learn-zf2-acl/listresourcesandrights'); ?>', function(jsondata) {
        // setup resource and right select by clear first
        document.getElementById('resource').selectedIndex = -1;
        document.getElementById('right').selectedIndex = -1;

        var resource = document.getElementById('resource');
        for (i = 0; i < resource.length; i++) {
            for (var key in jsondata.resources) {
                var resourceInnerHTML = resource[i].innerHTML;
                var resourceJsonData  = jsondata.resources[key];

                if (resourceInnerHTML.trim() == resourceJsonData) {
                    resource.options[i].selected = true;
                }
            }
        }

        var right = document.getElementById('right');
        for (i = 0; i < right.length; i++) {
            for (var key in jsondata.rights) {
                var rightInnerHTML = right[i].innerHTML;
                var rightJsonData  = jsondata.rights[key];

                if (rightInnerHTML.trim() == rightJsonData) {
                    right.options[i].selected = true;
                }
            }
        }


    }, params);
}
</script>
